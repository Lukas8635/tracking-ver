{
  "version": 2,
  "builds": [
    {
      "src": "vercel-server.js",
      "use": "@vercel/node",
      "config": { "maxDuration": 300 }
    }
  ],
  "routes": [
    { "src": "/", "dest": "/vercel-server.js" },
    { "src": "/analyze", "dest": "/vercel-server.js" },
    { "src": "/(.*)", "dest": "/vercel-server.js" }
  ],
  "functions": {
    "vercel-server.js": {
      "runtime": "nodejs22.x"
    }
  }
}
